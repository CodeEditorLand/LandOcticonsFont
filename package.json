{
  "name": "vscode-octicons-font",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist && rm -rf _temp",
    "compile": "npm run clean",
    "dev": "npm run compile && npm run fonts -- --html true --htmltp src/template/preview.hbs",
    "build": "npm run compile && npm run fonts -- --html false",
    "fonts": "icon-font-generator src/icons/*.svg -o dist -n octicons -p octicon --csstp src/template/styles.hbs --height 1000 --center --codepoints src/template/mapping.json --descent 180 --json false --types 'ttf'",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "devDependencies": {
    "icon-font-generator": "2.1.11",
    "typescript-esbuild": "0.3.1"
  },
  "type": "module",
  "private": "false",
  "publishConfig": {
    "access": "public"
  }
}
